.time-picker
  position: relative
  a
    font-weight: bold

  .time-picker-dialog
    z-index: 9999 !important
    display: block
    position: absolute
    width: 240px
    padding: 8px
    border-width: 1px
    border-style: solid
    border-color: #CCC #CCC #BBB
    background-color: white
    box-shadow: 0px 5px 15px -5px rgba(0, 0, 0, 0.5)
